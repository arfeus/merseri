<div class="container">
	<br  />
	<h2><%= current_user.name %> all activities</h2>
	<br  />
	<ul>
	<% @actings_cu.each do |a| %>
	  <li>
	  "<%= Activity.find(a.activity_id).name.capitalize %>" started <%= a.start.to_formatted_s(:short) %> duration <%= Time.at(a.stop - a.start).utc.strftime("%H:%M:%S:") %>
    </li>
	<% end %>
	</ul>
	<br  />
	<br  />
	<%= link_to "Action", action_path, class: "btn btn-lg btn-primary btn-block" %>

</div>